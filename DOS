   "source": [
    "m = 9.109e-31; \n",
    "hbar = 6.626e-34/2.0/np.pi; \n",
    "size = 1e-9; \n",
    "Emax = 2.0; \n",
    "k0 = np.pi/size; #\n",
    "kF = np.sqrt(Emax*1.602e-19*2*m)/hbar; \n",
    "N = int(np.pi*kF**2/k0**2); \n",
    "x = np.linspace(-N*k0,N*k0,2*N,endpoint=False); \n",
    "y = np.linspace(-N*k0,N*k0,2*N,endpoint=False); \n",
    "kx,ky = np.meshgrid(x,y);\n",
    "En = (hbar*x*1e9)**2/(2*m); "
   ]
